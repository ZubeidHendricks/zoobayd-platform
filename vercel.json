{
    "version": 2,
    "builds": [
        {
            "src": "packages/frontend/package.json",
            "use": "@vercel/next",
            "config": { 
                "nodeVersion": "18.x" 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "packages/frontend/$1"
        }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": "nextjs",
    "outputDirectory": "packages/frontend/.next"
}