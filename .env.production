# Vercel Production Configuration

# MongoDB Connection
MONGODB_URI=mongodb+srv://<username>:<password>@cluster.mongodb.net/zoobayd

# Authentication
JWT_SECRET=long_and_secure_random_string_for_production
JWT_EXPIRY=24h

# API Keys
OPENAI_API_KEY=your_production_openai_api_key

# Application Configuration
NODE_ENV=production
PORT=3001

# Frontend URL
FRONTEND_URL=https://zoobayd.com

# Blockchain RPC Endpoints
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_project_id
POLYGON_RPC_URL=https://polygon-rpc.com
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Feature Flags
ENABLE_CONTRACT_GENERATION=true
ENABLE_ML_OPTIMIZATION=true
ENABLE_BLOCKCHAIN_INTEGRATION=true